{
  "name": "design-docs",
  "version": "0.10.0",
  "files": [
    "dist"
  ],
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "peerDependencies": {
    "react": "*",
    "react-router-dom": "^6.0.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "json": "^11.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "typescript": "^4.5.4",
    "vite": "^2.7.13"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/erikpukinskis/design-docs.git"
  },
  "homepage": "https://github.com/erikpukinskis/design-docs",
  "scripts": {
    "example": "vite serve example --config vite.config.js",
    "build": "rm -rf dist; npm run build:vite; npm run build:types; npm run build:files",
    "build:vite": "vite build",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir dist --skipLibCheck",
    "build:files": "mv dist/index.d.ts dist/index.umd.d.ts",
    "check:types": "tsc --noEmit --skipLibCheck; if [ $? -eq 0 ]; then echo 8J+OiSBUeXBlcyBhcmUgZ29vZCEKCg== | base64 -d; fi",
    "bump": "./bump.sh"
  }
}
